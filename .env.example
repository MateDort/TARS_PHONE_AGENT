# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890
TARGET_PHONE_NUMBER=+1234567890

# Webhook Configuration
# Port 5002 URL (for webhooks)
WEBHOOK_BASE_URL=https://your-ngrok-url.ngrok-free.app
# Port 5001 URL (for WebSocket - use wss:// protocol)
WEBSOCKET_URL=wss://your-websocket-ngrok-url.ngrok-free.app
# Make sure these are correct:
WEBHOOK_PORT=5002
WEBSOCKET_PORT=5001

# Audio Configuration (optional)
AUDIO_TEMP_DIR=/tmp/phone_agent_audio

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Auto Call Configuration
AUTO_CALL=false

# Language Configuration
LANGUAGE=english

# Messaging Configuration
ENABLE_SMS=true
ENABLE_WHATSAPP=true

# TARS Personality Configuration (Dynamically Editable)
HUMOR_PERCENTAGE=65
HONESTY_PERCENTAGE=95
PERSONALITY=normal
NATIONALITY=English

# Delivery Method Configuration
REMINDER_DELIVERY=call
CALLBACK_REPORT=call

# Agent Hub Configuration
TARGET_NAME=Your Name
MAX_CONCURRENT_SESSIONS=10
